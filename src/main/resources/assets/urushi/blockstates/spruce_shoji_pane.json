{
  "multipart": [
    {
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_spruce_noside",
        "y": 270
      }
    }
  ]
}