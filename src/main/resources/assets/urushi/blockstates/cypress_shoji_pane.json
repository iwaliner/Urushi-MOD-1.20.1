{
  "multipart": [
    {
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "urushi:block/shouji_pane_cypress_noside",
        "y": 270
      }
    }
  ]
}